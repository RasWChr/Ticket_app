      <image>
         <Image url="@Rat.png" />
      </image>
   </ImageView>
   <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Username" wrappingWidth="272.13671875">
      <font>
         <Font size="14.0" />
      </font>
   </Text>
   <TextField fx:id="txtUN" prefHeight="25.0" prefWidth="269.0" />
   <Text fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="Password" wrappingWidth="272.13671875">
      <font>
         <Font size="14.0" />
      </font>
   </Text>
   <TextField fx:id="txtPsWrd" prefHeight="25.0" prefWidth="269.0" />
   <Label />
    <Button fx:id="btnLogIn" onAction="#onLogInClick" prefHeight="51.0" prefWidth="175.0" style="-fx-background-color: #369F97;" text="Log In" textFill="WHITE">
